{
  "name": "lacona-phrase-datetime",
  "version": "0.2.0",
  "description": "Enable Lacona to parse natural language dates, times, and both together ",
  "main": "lib/index.js",
  "scripts": {
    "lint": "npm run lint:lib && npm run lint:test",
    "lint:lib": "jshint --config node_modules/lacona-util-jshint/lib.json lib",
    "lint:test": "jshint --config node_modules/lacona-util-jshint/test.json test",
    "test": "mocha test",
    "validate": "npm run lint && npm test",
    "cover": "istanbul cover _mocha test -- -R dot && istanbul report text",
    "coveralls": "istanbul cover _mocha test --report lcovonly -- -R dot && cat coverage/lcov.info | coveralls && rimraf ./coverage",
    "prepublish": "npm run validate"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/brandonhorst/lacona-phrase-datetime.git"
  },
  "keywords": [
    "lacona",
    "date",
    "time",
    "datetime",
    "phrase"
  ],
  "author": "@brandonhorst",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/brandonhorst/lacona-phrase-datetime/issues"
  },
  "homepage": "https://github.com/brandonhorst/lacona-phrase-datetime",
  "devDependencies": {
    "chai": "^1.10.0",
    "chai-datetime": "^1.3.1",
    "coveralls": "^2.11.2",
    "istanbul": "^0.3.5",
    "jshint": "^2.6.0",
    "lacona-util-jshint": "^0.2.0",
    "mocha": "^2.1.0",
    "moment": "^2.9.0"
  },
  "dependencies": {
    "lacona": "^0.16.0"
  }
}
